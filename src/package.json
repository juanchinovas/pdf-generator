{
  "name": "@sunacchi/pdf-generator",
  "version": "1.3.0",
  "description": "NodeJs plugin to generate PDF from HTML template using Vue and puppeteer",
  "main": "index.js",
  "scripts": {
    "start": "cd ../demo/ && npm i && npm start",
    "build2npm": "cd ../compiled/ && npm i && npm run run-gulp",
    "config": "npm run build2npm && npm run start",
    "pack": "npm test && cd ../compiled/ && npm i && npm run run-pack",
    "publish": "npm run lint && npm run pack && cd ../compiled/ && npm run run-publish",
    "test": "jest",
    "config-dev": "npm i && npm run config",
	"lint": "eslint ."
  },
  "keywords": [
    "pdf",
    "template",
    "html",
    "vueJs",
    "puppeteer",
    "pdf-creator",
    "nodejs",
    "html-pdf-generator",
    "pdf-generator"
  ],
  "author": {
    "name": "juanchinovas",
    "email": "juanchinovas@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanchinovas/pdf-generator.git"
  },
  "bugs": {
    "url": "https://github.com/juanchinovas/pdf-generator/issues"
  },
  "publishConfig": {
	"access": "public"
  },
  "homepage": "https://github.com/juanchinovas/pdf-generator",
  "dependencies": {
    "puppeteer-core": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1"
  }
}
