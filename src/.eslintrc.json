{
  "env": {
    "node": true,
    "es2021": true,
	"jest": true
  },
  "extends": "eslint:recommended",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
		"semi": ["error", "always"],
        "quotes": ["error", "double"],
		"indent": [ "error", "tab"],
        "linebreak-style": [ "error", "unix" ],
		"no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
