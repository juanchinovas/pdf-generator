{
  "name": "pdf-processor",
  "version": "1.0.0",
  "description": "Generate PDF from html app using puppeteer and Vuejs",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "html-pdf-generator": "file:../compiled/npm"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "PORT": 3000,
      "FILE_DIR": "./temp",
      "PDF_DIR": "./temp/pdfs",
      "URL_BROWSER": "/opt/google/chrome/google-chrome",
      "TEMPLATE_DIR": "./templates",
      "BROWSER_NAME": "chrome"
    }
  }
}
